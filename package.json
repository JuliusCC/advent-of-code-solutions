{
	"name": "advent-of-code-solutions",
	"version": "0.4.0",
	"homepage": "https://JuliusCC.github.io/advent-of-code-solutions",
	"repository": {
		"type": "git",
		"url": "https://github.com/JuliusCC/advent-of-code-solutions"
	},
	"private": false,
	"dependencies": {
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-scripts": "1.0.17",
		"styled-components": "^2.2.4"
	},
	"devDependencies": {
		"gh-pages": "^1.1.0",
		"husky": "^0.14.3",
		"lint-staged": "^4.3.0",
		"prettier": "^1.9.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"deploy": "npm run build && gh-pages -d build",
		"precommit": "lint-staged && npm run deploy",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,json,css}": [
			"prettier --single-quote --use-tabs --no-semi --write",
			"git add"
		]
	}
}
